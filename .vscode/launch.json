{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Docker Test",
            "type": "al",
            "request": "launch",
            "server": "https://BC-TEST/",
            "serverInstance": "BC",
            "authentication": "UserPassword",
            //"startupObjectId": 22,
            "startupObjectType": "Page",
            "breakOnError": "All",
            "launchBrowser": true,
            "enableLongRunningSqlStatements": true,
            "enableSqlInformationDebugger": true,
            "tenant": "default",
            "schemaUpdateMode": "ForceSync"
        },
        {
            "name": "Launch Docker Test Debug",
            "type": "al",
            "request": "attach",
            "server": "https://BC-TEST/",
            "serverInstance": "BC",
            "authentication": "UserPassword",
            "breakOnError": "All",
            "enableLongRunningSqlStatements": true,
            "enableSqlInformationDebugger": true,
            "tenant": "default"
        }
    ]
}